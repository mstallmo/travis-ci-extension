{
  "short_name": "Travis Ci Watcher",
  "name": "Travis Ci Build Watcher",
  "manifest_version": 2,
  "version": "1.0",
  "icons": {
      "24": "pizza-truck.png"
  },
  "permissions": [
      "notifications",
      "background"
  ],
  "browser_action": {
      "default_popup": "index.html",
      "default_title": "Travis Ci Build Watcher"
  },
  "background": {
      "scripts": ["/static/js/background.js"]
  },
  "permissions": [
      "https://code.jquery.com/*",
      "https://cdnjs.cloudflare.com/ajax/libs/popper.js/*",
      "https://maxcdn.bootstrapcdn.com/bootstrap/*"
  ]
}
